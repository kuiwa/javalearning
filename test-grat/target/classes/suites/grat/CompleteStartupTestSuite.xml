<suite name="CompleteStartup" parallel="false" rbsGeneration="gen2" standard="GRAT">
	<listeners>
		<listener class-name="com.ericsson.msran.jcat.MsranTestNgListener" />
	</listeners>

 	<test name="Complete Startup, 1 GsmSector with 4 Trxs">
		<classes>
 			<class name="com.ericsson.msran.test.grat.completestartup.CompleteStartupTest">
				<methods>
   					<include name="completeStartup" >
						<parameter name="testId" value="GRAT000025" />
						<parameter name="description" value="Complete startup of all MOs, 1 GsmSector with 4 Trxs." />
                        <parameter name="numOfTGs" value="1" />
						<parameter name="numOfTrxesPerTG" value="4" />
						<parameter name="QA" value="CONFIGURE_AND_UPGRADE" />
						<parameter name="FAJ" value="N/A" />
						<parameter name="Test slogan" value="Complete startup of 1 GsmSector with 4 Trxs." />
                        <parameter name="NODEUC" value="UC513.N" />
						<parameter name="NODEUC" value="UC602.N" />
						<parameter name="NODEUC" value="UC607.N" />
						<parameter name="NODEUC" value="UC627.N" />
                        <parameter name="NODEUC" value="UC646.N" />
					</include>
				</methods>
			</class>
		</classes>
	</test>	
	<test name="Complete Startup, 4 GsmSectors with 1 Trx in each GsmSector">
		<classes>
 			<class name="com.ericsson.msran.test.grat.completestartup.CompleteStartupTest">
				<methods>
   					<include name="completeStartup" >
						<parameter name="testId" value="GRAT000025" />
						<parameter name="description" value="Complete startup of all MOs, 4 GsmSectors with 1 Trx in each GsmSector." />
                        <parameter name="numOfTGs" value="4" />
						<parameter name="numOfTrxesPerTG" value="1" />
						<parameter name="QA" value="CONFIGURE_AND_UPGRADE" />
						<parameter name="FAJ" value="N/A" />
						<parameter name="Test slogan" value="Complete startup of 4 GsmSectors with 1 Trx in each GsmSector." />
                        <parameter name="NODEUC" value="UC513.N" />
						<parameter name="NODEUC" value="UC602.N" />
						<parameter name="NODEUC" value="UC607.N" />
						<parameter name="NODEUC" value="UC627.N" />
                        <parameter name="NODEUC" value="UC646.N" />
					</include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="Complete Startup, 2 GsmSectors with 2 Trxs in each GsmSector">
		<classes>
 			<class name="com.ericsson.msran.test.grat.completestartup.CompleteStartupTest">
