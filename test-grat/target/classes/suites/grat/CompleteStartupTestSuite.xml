						<parameter name="QA" value="CONFIGURE_AND_UPGRADE" />
						<parameter name="FAJ" value="N/A" />
						<parameter name="Test slogan" value="Complete startup of 2 GsmSectors with 2 Trxs in each GsmSector." />
                        <parameter name="NODEUC" value="UC513.N" />
						<parameter name="NODEUC" value="UC602.N" />
						<parameter name="NODEUC" value="UC607.N" />
						<parameter name="NODEUC" value="UC627.N" />
                        <parameter name="NODEUC" value="UC646.N" />
					</include>
				</methods>
			</class>
		</classes>
	</test>

