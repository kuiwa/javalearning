<suite name="SoftwareUpgradeTestSuite" parallel="false" preserve-order="true" rbsGeneration="gen2" standard="GRAT">
  <listeners>
    <listener class-name="com.ericsson.msran.jcat.MsranTestNgListener" />
    <listener class-name="com.ericsson.msran.test.grat.testhelpers.GratTestListener"/>
  </listeners>
  
  <test name="GRAT software upgrade fetch and unpack to-state upgrade package" >
    <classes>
      <class name="com.ericsson.msran.test.grat.softwareupgrade.SoftwareUpgradeTest" >
        <methods>
          <include name="prepareUpgradePackageStep" >
            <parameter name="testId" value="LTE00220829"/>
            <parameter name="description" value="fetch and unpack to-state upgrade package to sftp server path"/>
          </include>
        </methods>
      </class>
    </classes>
  </test>
   
  <test name="GRAT software upgrade create and prepare upgrade package" >
    <classes>
      <class name="com.ericsson.msran.test.grat.softwareupgrade.SoftwareUpgradeTest" >
        <methods>
          <include name="createAndPrepareUpgradePackage" >
          	<parameter name="testId" value="LTE00214467"/>
          	<parameter name="description" value="createUpgradePackage in SwM=1"/>
            <parameter name="QA" value="CONFIGURE_AND_UPGRADE"/>            
            <parameter name="NODEUC" value="NodeUC416.N.1"/>
            <parameter name="NODEUC" value="NodeUC416.N.3"/>            
          </include>
        </methods>
      </class>
    </classes>
  </test>

