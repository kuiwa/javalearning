		<methods>
		  <include name="makeReoveryFromResetSoTrxc">
			<parameter name="testId" value="LTE00231242"/>
			<parameter name="description" value="Baseband internal fault is detected and reset SO TRXC" />
            <parameter name="QA" value="CONFIGURE_AND_UPGRADE" />
