<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="ueConfiguration"
          class="com.ericsson.msran.trafficmodel.configuration.ue.UeConfiguration"> 
    </bean> 
    
    
     <bean id="lteUeConfiguration"
          class="com.ericsson.msran.trafficmodel.configuration.ue.LteUeConfiguration"
          parent="defaultUeConfiguration">
        <property name="cellId" value="228"/>
    </bean>
    
     <bean id="aeroflexUeConfiguration"
          class="com.ericsson.msran.trafficmodel.configuration.ue.AeroflexUeConfiguration"
          parent="defaultLteUeConfiguration">
        <property name="ulUeCategory" value="2"/>
        <property name="dlUeCategory" value="2"/>
        <!-- <property name="attachTimeoutPerUeSec" value="0"/> -->
    </bean> 
    
    <bean id="nasConfigCapabilityOptions"
          class="com.ericsson.msran.trafficmodel.configuration.ue.NasConfigCapabilityOptions"
          parent="defaultAeroflexNasConfigCapabilityOptions">
        <property name="encryptionCapability" value="224"/>
        <property name="integrityCapability" value="224"/>
        <!--<property name="nasSecurityOption" value="-1"/> -->
    </bean>
    
    
    

</beans>

